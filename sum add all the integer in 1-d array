#include <stdio.h>

int main()
{
    int output=0;
    int i,j;
    int array[2][3]={
                    
                    {1,2,3},
                    {4,5,6}
    };
    for(i=0;i<2;i++){
        for(j=0;j<3;j++){
        output=output +array[i][j];
        printf("%d \n",output);
        }
    }
    return 0;
}
